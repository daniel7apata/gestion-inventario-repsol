<Window x:Class="Prensentacion.Eliminar_usuarios"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prensentacion"
        mc:Ignorable="d"
        Title="Eliminar_usuarios" Height="228" Width="290">
    <Grid Margin="0,0,22,14">
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Margin="70,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="146" Height="28" TextChanged="txtUsuario_TextChanged"/>
        <Label Content="Ingrese usuario a eliminar" HorizontalAlignment="Left" Margin="70,54,0,0" VerticalAlignment="Top"/>
        <Button Content="Eliminar" HorizontalAlignment="Left" Margin="105,143,0,0" VerticalAlignment="Top" Click="Button_Click" Height="32" Width="76"/>

    </Grid>
</Window>
