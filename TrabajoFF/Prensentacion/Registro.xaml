<Window x:Class="Prensentacion.Registro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prensentacion"
        mc:Ignorable="d"
        Title="Registro" Height="365" Width="457">
    <Grid Margin="0,0,10,8">
        <Label Content="Usuario" HorizontalAlignment="Left" Margin="76,69,0,0" VerticalAlignment="Top"/>
        <Label Content="Contraseña" HorizontalAlignment="Left" Margin="76,153,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Center" Margin="0,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="295" TextChanged="TextBox_TextChanged" Height="31"/>
        <TextBox x:Name="txtContrasenia" HorizontalAlignment="Center" Margin="0,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="295" Height="28"/>
        <Button Content="Registrar" HorizontalAlignment="Center" Margin="0,248,0,0" VerticalAlignment="Top" Height="33" Width="118" Click="Button_Click"/>

    </Grid>
</Window>
